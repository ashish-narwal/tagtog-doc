const replaceRegex = function(regex){
	return function(str){
		return str.match(regex);
	}
}

const headingRegex = /\n(#+\s*)(.*)/g;

const getHeadings = replaceRegex(headingRegex);

const getParsedHeadings = function(str) {
    return getHeadings(str);
}

