---
layout: null
sitemap: false
---

var store = [
  {%- for doc in site.pages -%}
    {% if doc.title contains '(old link)' or doc.title == null or doc.title == 'null' or doc.title contains '404'%}{% continue %}{% endif %}
    {%- assign head = doc.content | strip_html | smartify  | split: "###" -%}
          {% for member in head %}
            {% if forloop.first == true %} 
            {"title": {{doc.title | jsonify}},
              "section": false,
            {% else %}
            {% assign title = member | split: "\n\n\n" %}
            {% assign check1 = title[0] | split: "\n\n" %}
            {% assign check2 = check1[0] | split: "\n" %}
            
            {"title": {{check2[0] | jsonify }},
              "section": true,
            {% endif %}
            "excerpt" : {{member | replace:"</p>", " " | 
              replace:"</h1>", " " | 
              replace:"</h2>", " " |
              replace:"###", " " |
              replace:"##", " " |
            strip_html | strip_newlines | smartify | jsonify }},
            "url": {{ doc.url | absolute_url | jsonify }}
          }
        {%- unless forloop.last and l -%}, {%- endunless -%}
        {% endfor %}
  {%- endfor -%}
]

